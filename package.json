{
  "name": "twin-macro-autocomplete-vscode",
  "displayName": "twin.macro Auto Complete",
  "version": "0.0.4",
  "publisher": "DennisVash",
  "repository": {
    "type": "git",
    "url": "https://github.com/denvash/twin.macro-autocomplete-vscode"
  },
  "bugs": {
    "url": "https://github.com/denvash/twin.macro-autocomplete-vscode/issues"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "twin.macro": "^1.0.0",
    "typescript": "^3.8.3"
